{
  "name": "devops-book-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test:unit": "jest --runInBand",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "test:frontend": "jest tests/frontend --runInBand"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "utils/bryanUtil.js",
      "public/js/bryan.js",
      "models/book.js",
      "utils/retrieveBooksUtil.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "utils/bryanUtil.js": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      },
      "public/js/bryan.js": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      },
      "models/book.js": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      },
      "utils/retrieveBooksUtil.js": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "devDependencies": {
    "@playwright/test": "^1.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^6.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chewingbytes/devops-book-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chewingbytes/devops-book-project/issues"
  },
  "homepage": "https://github.com/chewingbytes/devops-book-project#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-status-monitor": "^1.3.4",
    "winston": "^3.19.0"
  }
}
